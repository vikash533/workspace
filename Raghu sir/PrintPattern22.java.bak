import java.util.Scanner;
class PrintPattern22
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner (System.in);
		System.out.println("Enter the value of n");
		int n=sc.nextInt();
		int x=1;
		for (int i=n;i>0 ;i-- )
		{
			for (int j=1;j<=n ;j++ )
			{
				System.out.print(x+"");

			}
			System.out.println();
			x++;
		}
		
	}
}
