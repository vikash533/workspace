import java.util.Scanner;
class PrintPattern32
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner (System.in);
		System.out.println("Enter the value of n");
		int n=sc.nextInt();
		
		for (int i=1;i<=n ;i++)
		{
			int x=i;
			for (int j=1;j<=i;j++ )
			{
				System.out.print(x+" ");
				x=x+n-j;
				
			}
			System.out.println();
			
		}
		
	}
}
